{
  "server/**/*.js": [
    "cd server && npx eslint --fix",
    "cd server && npx prettier --write"
  ],
  "client-angular/src/**/*.ts": [
    "cd client-angular && npx eslint --fix",
    "cd client-angular && npx prettier --write"
  ],
  "client-angular/src/**/*.html": ["cd client-angular && npx prettier --write"],
  "client-angular/src/**/*.{scss,css}": [
    "cd client-angular && npx prettier --write"
  ],
  "*.json": ["npx prettier --write"]
}
