{
  "name": "date-night-app",
  "version": "1.0.0",
  "description": "DateNight.io application",
  "workspaces": [
    "client-angular",
    "server"
  ],
  "scripts": {
    "preinstall": "node scripts/disable-husky-in-ci.js",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd server && npm run dev",
    "start:client": "node scripts/start-client.js",
    "install-all": "npm install && cd server && npm install && cd ../client-angular && npm install",
    "install-missing": "node scripts/install-missing-deps.js",
    "fix-csp": "node scripts/fix-csp-issues.js",
    "fix-csp:restart": "node scripts/fix-csp-issues.js --restart",
    "test": "jest",
    "test:server": "jest --selectProjects server-unit server-integration",
    "test:server:unit": "jest --selectProjects server-unit",
    "test:server:integration": "jest --selectProjects server-integration",
    "test:server:performance": "jest --selectProjects server-performance",
    "test:client": "cd client-angular && npm test",
    "test:client:coverage": "cd client-angular && npm run test:coverage",
    "test:all": "npm run test:server && npm run test:client",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "seed": "node scripts/seed.js",
    "mongodb": "node scripts/ensure-mongodb.js",
    "fix-mongodb": "node scripts/fix-mongodb-issues.js",
    "check-mongodb": "node scripts/check-mongodb-permissions.js",
    "dev": "bash ./fix-project.sh && concurrently \"npm run start:server\" \"npm run start:client\"",
    "setup:mongodb": "npm run check-mongodb && npm run fix-mongodb",
    "audit-fix": "npm audit fix --force",
    "make-executable": "bash ./make-scripts-executable.sh",
    "fix-all": "bash ./fix-project.sh",
    "check-node": "node scripts/check-node-version.js",
    "update-packages": "node scripts/update-packages.js",
    "analyze:security": "npm audit && cd server && npm audit && cd ../client-angular && npm audit",
    "analyze:performance": "npm run test:server:performance",
    "analyze:coverage": "npm run test:coverage && open coverage/lcov-report/index.html",
    "fix:workflow-issues": "node scripts/fix-workflow-issues.js",
    "prepare": "[ -n \"$CI\" ] || husky",
    "lint": "npm run lint:server && npm run lint:client",
    "lint:server": "cd server && npm run lint || true",
    "lint:client": "cd client-angular && npm run lint || true",
    "lint:fix": "npm run lint:server:fix && npm run lint:client:fix",
    "lint:server:fix": "cd server && npm run lint:fix || true",
    "lint:client:fix": "cd client-angular && npm run lint:fix || true",
    "format": "npm run format:server && npm run format:client",
    "format:server": "cd server && npm run format",
    "format:client": "cd client-angular && npm run format",
    "format:check": "npm run format:server:check && npm run format:client:check",
    "format:server:check": "cd server && npm run format:check",
    "format:client:check": "cd client-angular && npm run format:check",
    "format:all": "npm run format && npm run lint:fix"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "angular-mocks": "^1.8.3",
    "concurrently": "^8.2.2",
    "eslint-plugin-sonarjs": "3.0.2",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "15.5.1",
    "npm-check": "6.0.1",
    "xml2js": "0.6.2"
  },
  "dependencies": {
    "@octokit/rest": "21.1.1",
    "@popperjs/core": "2.11.8",
    "child_process": "^1.0.2",
    "dotenv": "^16.3.1",
    "fs-extra": "11.2.0",
    "helmet": "^8.1.0",
    "mongoose": "^8.2.3",
    "path": "^0.12.7",
    "semver": "^7.5.4"
  },
  "overrides": {
    "semver": "^7.5.4",
    "tough-cookie": "^4.1.3",
    "word-wrap": "^1.2.5",
    "vite": "^6.2.7",
    "cookie": "^0.7.2",
    "got": "^12.1.0",
    "http-proxy-middleware": "^3.0.5",
    "rimraf": "^5.0.5",
    "glob": "^10.3.10",
    "@eslint/config-array": "^0.20.0",
    "@eslint/object-schema": "^2.1.6"
  },
  "engines": {
    "node": "22.14.0",
    "npm": "10.9.2"
  }
}
