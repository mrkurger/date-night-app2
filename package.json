{
  "name": "date-night-app",
  "version": "1.0.0",
  "description": "DateNight.io application",
  "workspaces": [
    "client-angular",
    "server"
  ],
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd server && npm run dev",
    "start:client": "cd client-angular && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client-angular && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed": "node scripts/seed.js",
    "mongodb": "mongod --dbpath ./data/db",
    "dev": "concurrently \"npm run mongodb\" \"npm run start:server\" \"npm run start:client\""
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "angular-mocks": "^1.8.3"
  },
  "dependencies": {
    "helmet": "^8.1.0",
    "mongoose": "^8.13.2"
  }
}
