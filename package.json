{
  "name": "date-night-app",
  "version": "1.0.0",
  "description": "Date Night Application",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd server && pnpm start",
    "start:client": "cd client-angular && npm start",
    "install:all": "pnpm install && cd server && pnpm install && cd ../client-angular && npm install",
    "build": "cd client-angular && npm run build",
    "test": "cd client-angular && npm test",
    "lint": "cd client-angular && npm lint",
    "prepare": "[ -n \"$CI\" ] || husky install",
    "docs:ensure": "node .github/scripts/ensure-docs.js",
    "docs:update": "npm run docs:ensure",
    "install-missing": "node scripts/install-missing-deps.js",
    "fix-tests": "node scripts/combine-test-reports.js",
    "analyze:security": "npm audit"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.12",
    "@angular/compiler-cli": "^19.2.11",
    "@types/jest": "29.5.14",
    "concurrently": "^8.2.2",
    "eslint": "^9.0.0",
    "husky": "9.1.7",
    "jasmine-core": "5.7.1",
    "jest": "29.7.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@angular/common": "19.2.11",
    "@angular/compiler": "^19.2.11",
    "@angular/core": "19.2.11",
    "@angular/forms": "19.2.11",
    "@angular/platform-browser": "19.2.11",
    "@angular/router": "19.2.11",
    "@fontsource/inter": "5.2.5",
    "@fontsource/poppins": "5.2.6",
    "@nebular/eva-icons": "11.0.1",
    "@nebular/theme": "11.0.1",
    "@swimlane/ngx-charts": "22.0.0",
    "dotenv": "16.5.0",
    "eva-icons": "1.1.3",
    "fs-extra": "11.3.0",
    "helmet": "8.1.0",
    "leaflet": "1.9.4",
    "mongoose": "8.15.0",
    "rxjs": "~7.8.0",
    "semver": "7.7.2",
    "ts-node": "10.9.2"
  }
}
