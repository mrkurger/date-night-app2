{
  "name": "date-night-app",
  "version": "1.0.0",
  "description": "Date Night Application",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd server && pnpm start",
    "start:client": "cd client-angular && ./node_modules/.bin/ng serve --project=client-angular",
    "install:all": "pnpm install && cd server && pnpm install && cd ../client-angular && pnpm install",
    "build": "cd client-angular && pnpm build",
    "test": "cd client-angular && pnpm test",
    "lint": "cd client-angular && pnpm lint",
    "prepare": "[ -n \"$CI\" ] || husky install",
    "docs:ensure": "node .github/scripts/ensure-docs.js",
    "docs:update": "npm run docs:ensure",
    "install-missing": "node scripts/install-missing-deps.js",
    "fix-tests": "node scripts/combine-test-reports.js",
    "analyze:security": "npm audit",
    "mcp:start": "node scripts/start-mcp-services.js",
    "mcp:stop": "node scripts/mcp-memory-server.js stop",
    "mcp:sync": "node scripts/mcp-knowledge-sync.js",
    "mcp:status": "node scripts/mcp-memory-server.js status"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.0.0",
    "@angular/cli": "^18.2.19",
    "@angular/compiler-cli": "^18.2.13",
    "@playwright/test": "1.52.0",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.17",
    "@types/node": "22.15.29",
    "@types/react": "19.1.6",
    "concurrently": "^8.2.2",
    "eslint": "^9.0.0",
    "husky": "9.1.7",
    "jasmine-core": "5.7.1",
    "jest": "29.7.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "mem0ai": "1.0.39",
    "snyk": "1.1297.1",
    "typescript": "5.8.3",
    "typescript-language-server": "4.3.4"
  },
  "dependencies": {
    "@angular/animations": "^19.2.11",
    "@angular/common": "^19.2.11",
    "@angular/compiler": "^19.2.11",
    "@angular/core": "19.2.13",
    "@angular/forms": "^19.2.11",
    "@angular/platform-browser": "^19.2.11",
    "@angular/platform-browser-dynamic": "^19.2.11",
    "@angular/router": "^19.2.11",
    "@angular/service-worker": "^19.2.11",
    "@fontsource/inter": "5.2.5",
    "@fontsource/poppins": "5.2.6",
    "@fortawesome/angular-fontawesome": "1.0.0",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@modelcontextprotocol/server-memory": "2025.4.25",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@nebular/theme": "11.0.1",
    "@next/third-parties": "15.3.3",
    "@radix-ui/react-select": "2.2.5",
    "@swimlane/ngx-charts": "22.0.0",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "16.5.0",
    "framer-motion": "12.15.0",
    "fs-extra": "11.3.0",
    "helmet": "8.1.0",
