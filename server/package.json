{
  "name": "date-night-server",
  "version": "1.0.0",
  "description": "Backend for Date Night App",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config server.js",
    "dev": "npx nodemon -r dotenv/config server.js",
    "test": "NODE_ENV=test npx jest --config=jest.config.cjs",
    "test:esm": "NODE_ENV=test npx jest --config=jest.config.cjs",
    "predev": "node ../scripts/ensure-mongodb.js",
    "start-client": "node ./scripts/start-client.js",
    "audit-fix": "npm audit fix --force",
    "lint": "eslint . --fix --max-warnings=50",
    "lint:fix": "eslint . --fix --max-warnings=9999",
    "format": "prettier --write \"**/*.{js,json}\"",
    "format:check": "prettier --check \"**/*.{js,json}\""
  },
  "dependencies": {
    "argon2": "0.43.0",
    "axios": "1.9.0",
    "bcrypt": "6.0.0",
    "body-parser": "^2.2.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf-csrf": "^4.0.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.0",
    "express-validator": "^7.2.1",
    "file-type": "20.5.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "node-cache": "^5.1.2",
    "node-cron": "^4.0.3",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-reddit": "^1.1.0",
    "sharp": "0.34.1",
    "socket.io": "^4.7.5",
    "stripe": "^18.1.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.27.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.27.1",
    "@babel/preset-env": "7.27.2",
    "@eslint/eslintrc": "3.3.1",
    "babel-jest": "29.7.0",
    "core-js": "3.42.0",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.4.0",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "jest-cli": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-extended": "4.0.2",
    "jest-junit": "16.0.0",
    "mongodb-memory-server": "10.1.4",
    "nodemon": "^3.1.9",
    "prettier": "3.5.3",
    "supertest": "7.1.1"
  },
  "engines": {
    "node": "22.14.0",
