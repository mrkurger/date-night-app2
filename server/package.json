{
  "name": "date-night-server",
  "version": "1.0.0",
  "description": "Backend for Date Night App",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config dist/server.js",
    "dev": "npx nodemon -r dotenv/config dist/server.js",
    "build": "tsc -p tsconfig.build.json",
    "build:improved": "node ./scripts/improved-build.js",
    "build:simple": "node ./scripts/simple-build.js",
    "build:enhanced": "node ./scripts/enhanced-build.js",
    "build:ts-full": "node ./scripts/typescript-full-build.js",
    "build:compat": "node ./scripts/create-middleware-compatibility.js && node ./scripts/create-validator-compatibility.js",
    "build:validators": "node ./scripts/fix-validator-integration.js",
    "build:travel": "node ./scripts/fix-travel-components.js",
    "build:ts-imports": "node ./scripts/fix-typescript-imports.js",
    "build:safety": "node ./scripts/fix-safety-validator.js",
    "build:verification": "node ./scripts/fix-verification-controller.js",
    "build:verification-routes": "node ./scripts/fix-verification-routes.js",
    "build:users": "node ./scripts/fix-users-validator.js",
    "build:path-to-regexp": "node ./scripts/fix-path-to-regexp-errors.js",
    "build:path-to-regexp-ts": "node ./scripts/fix-path-to-regexp-errors-ts.js",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "validate": "node ./scripts/validate-server.js",
    "typecheck": "tsc --noEmit",
    "typecheck:module": "node ./scripts/type-check-module.js",
    "test": "NODE_ENV=test npx jest --config=jest.config.cjs",
    "test:watch": "NODE_ENV=test npx jest --watch --config=jest.config.cjs",
    "test:coverage": "NODE_ENV=test npx jest --coverage --config=jest.config.cjs",
    "test:path-to-regexp": "node ./scripts/test-path-to-regexp-ts.js",
    "test:ts-url": "NODE_ENV=test npx jest --testPathPattern=tests/unit/middleware/path-to-regexp-patch --config=jest.config.cjs",
    "predev": "node ../scripts/ensure-mongodb.js",
    "start-client": "node ./scripts/start-client.js",
    "test:db": "node ./scripts/test-db-connections.js",
    "audit-fix": "npm audit fix --force",
    "lint": "eslint . --fix --max-warnings=50",
    "lint:fix": "eslint . --fix --max-warnings=9999",
    "format": "prettier --write \"**/*.{js,json}\"",
    "format:check": "prettier --check \"**/*.{js,json}\""
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.15.19",
    "@types/validator": "^13.15.0",
    "argon2": "^0.43.0",
    "axios": "1.9.0",
    "bcrypt": "^6.0.0",
    "bip32": "^5.0.0-rc.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.7",
    "body-parser": "^2.2.0",
    "compression": "1.8.0",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "csrf-csrf": "4.0.2",
    "dotenv": "16.5.0",
    "ethers": "^6.14.3",
    "express": "^5.1.0",
    "express-mongo-sanitize": "2.2.0",
    "express-rate-limit": "7.5.0",
    "express-serve-static-core": "^0.1.1",
    "express-session": "^1.18.0",
    "express-slow-down": "^2.1.0",
    "express-validator": "^7.0.1",
    "file-type": "20.5.0",
    "glob": "^10.4.5",
    "helmet": "8.1.0",
    "hpp": "0.2.3",
    "http-errors": "^2.0.0",
    "isomorphic-dompurify": "^2.25.0",
    "jsonwebtoken": "9.0.2",
    "lru-cache": "^11.1.0",
    "mongoose": "8.15.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "node-cache": "^5.1.2",
    "node-cron": "^4.0.3",
    "node-fetch": "^4.0.0-beta.4",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^8.2.0",
    "prom-client": "^15.1.2",
    "sharp": "0.34.1",
    "socket.io": "4.8.1",
    "stripe": "^18.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tiny-secp256k1": "^2.2.3",
    "uuid": "11.1.0",
    "validator": "^13.15.0",
    "winston": "3.17.0",
    "winston-daily-rotate-file": "5.0.0",
    "xss": "^1.0.15",
    "xss-filters": "^1.2.7",
    "zod": "^3.25.7",
    "zod-validation-error": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.27.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.27.1",
    "@babel/preset-env": "7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "^9.26.0",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^4.17.22",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-jest": "29.7.0",
    "babel-plugin-transform-import-meta": "2.3.2",
    "core-js": "3.42.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.5.3",
    "globals": "^16.1.0",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "jest-cli": "29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-extended": "5.0.0",
    "jest-junit": "16.0.0",
    "joi": "^17.13.3",
    "mongodb-memory-server": "10.1.4",
    "nodemon": "^3.1.9",
    "prettier": "3.5.3",
    "supertest": "7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.3.1",
    "typescript": "^5.4.4"
  },
  "engines": {
    "node": "22.14.0",
    "npm": "10.9.2"
  },
  "overrides": {
    "semver": "^7.5.4",
    "tough-cookie": "^4.1.3",
    "word-wrap": "^1.2.5",
    "vite": "^6.2.7",
    "cookie": "^0.7.2",
    "@eslint/config-array": "0.20.0",
    "@eslint/object-schema": "2.1.6",
    "@octokit/plugin-paginate-rest": "11.4.2",
    "@octokit/request": "9.2.1",
    "@octokit/request-error": "6.0.1",
    "@octokit/core": "6.1.5",
    "@octokit/graphql": "8.0.1"
  }
}
