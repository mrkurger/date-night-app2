{
  "name": "date-night-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=client-angular",
    "build": "ng build client-angular",
    "watch": "ng build client-angular --watch --configuration development",
    "test": "./node_modules/.bin/ng test client-angular",
    "test:ci": "./node_modules/.bin/ng test client-angular --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint client-angular",
    "lint:fix": "ng lint client-angular --fix",
    "lint:html": "ng lint client-angular --file-type=html",
    "lint:ts": "ng lint client-angular --file-type=ts",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:all": "npm run lint:ts && npm run lint:html && npm run lint:styles",
    "format": "prettier --write \"src/**/*.{ts,html,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss}\"",
    "format:staged": "pretty-quick --staged",
    "e2e": "ng e2e client-angular",
    "docs:generate": "compodoc -p tsconfig.json -s -d docs"
  },
  "private": true,
