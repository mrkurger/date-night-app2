{
  "name": "client-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve client-angular",
    "build": "ng build client-angular",
    "watch": "ng build client-angular --watch --configuration development",
    "test": "ng test client-angular",
    "update-csp": "node src/csp-config.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
