{
  "name": "client-angular",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "NODE_OPTIONS=--max_old_space_size=8192 ng serve client-angular",
    "build": "NODE_OPTIONS=--max_old_space_size=8192 ng build client-angular",
    "build:prod": "NODE_OPTIONS=--max_old_space_size=8192 ng build client-angular --configuration production",
    "watch": "NODE_OPTIONS=--max_old_space_size=8192 ng build client-angular --watch --configuration development",
    "clean": "rm -rf node_modules && rm -f package-lock.json && rm -rf .angular/cache && rm -rf dist",
    "clean:install": "npm run clean && npm install",
    "clean:build": "npm run clean:install && npm run build",
    "test": "npx --node-options=--max-old-space-size=8192 ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "test:full": "npx --node-options=--max-old-space-size=8192 ng test --no-watch --no-progress",
    "update-csp": "node src/csp-config.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "npx --node-options=--max-old-space-size=8192 ng serve client-angular --port 0 & cypress run",
    "e2e:open": "npx --node-options=--max-old-space-size=8192 ng serve client-angular --port 0 & cypress open",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "lint:scss:check": "stylelint \"src/**/*.scss\"",
    "lint:scss:imports": "stylelint \"src/**/*.scss\" --config .stylelintrc.imports.json",
    "lint:scss:imports:fix": "stylelint \"src/**/*.scss\" --config .stylelintrc.imports.json --fix",
    "lint:scss:imports-only": "stylelint \"src/**/*.scss\" --config .stylelintrc.imports-only.json",
    "lint:scss:imports-only:fix": "stylelint \"src/**/*.scss\" --config .stylelintrc.imports-only.json --fix",
    "lint": "eslint . --fix --max-warnings=99999",
    "lint:check": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=99999",
    "format": "npx prettier --write \"src/**/*.{ts,html,scss,css,json}\"",
    "format:check": "npx prettier --check \"src/**/*.{ts,html,scss,css,json}\"",
    "format:all": "npm run format && npm run lint",
    "prepare": "husky",
    "compodoc": "npx compodoc -p tsconfig.json -s",
    "compodoc:build": "npx compodoc -p tsconfig.json -d ../docs/component-library"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.10",
    "@angular/cdk": "^19.1.4",
    "@angular/common": "19.2.10",
    "@angular/compiler": "19.2.10",
    "@angular/core": "19.2.10",
    "@angular/forms": "19.2.10",
    "@angular/material": "19.2.15",
    "@angular/platform-browser": "19.2.10",
    "@angular/platform-browser-dynamic": "19.2.10",
    "@angular/router": "19.2.10",
    "@angular/service-worker": "19.2.10",
    "@nebular/eva-icons": "15.0.0",
    "@nebular/theme": "15.0.0",
    "@ng-bootstrap/ng-bootstrap": "18.0.0",
    "@popperjs/core": "2.11.8",
    "@swimlane/ngx-charts": "22.0.0",
    "@types/leaflet": "1.9.17",
    "angularx-qrcode": "19.0.0",
    "bootstrap": "^5.3.2",
    "chai": "5.2.0",
    "chart.js": "4.4.9",
    "date-fns": "4.1.0",
    "emerald-ui": "2.14.3",
    "eva-icons": "1.1.3",
    "leaflet": "1.9.4",
    "leaflet-search": "4.0.0",
    "leaflet.heat": "0.2.0",
    "leaflet.markercluster": "1.5.3",
    "ngx-socket-io": "^4.5.1",
    "rxjs": "7.8.2",
    "socket.io-client": "4.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.11",
    "@angular-eslint/builder": "19.4.0",
    "@angular-eslint/eslint-plugin": "19.4.0",
    "@angular-eslint/eslint-plugin-template": "19.4.0",
    "@angular-eslint/schematics": "19.4.0",
    "@angular-eslint/template-parser": "19.4.0",
    "@angular/cli": "19.2.11",
    "@angular/compiler-cli": "19.2.10",
    "@compodoc/compodoc": "1.1.26",
    "@eslint/eslintrc": "^3.0.0",
    "@eslint/js": "^9.0.0",
    "@schematics/angular": "19.2.10",
    "@types/jasmine": "5.1.8",
    "@types/node": "22.15.17",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cypress": "14.3.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.2.6",
    "husky": "9.1.7",
    "jasmine": "5.7.1",
    "jasmine-core": "5.7.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "3.5.3",
    "stylelint": "16.18.0",
    "stylelint-config-recommended": "16.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-scss": "6.12.0",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": "22.14.0",
    "npm": "10.9.2"
  },
  "overrides": {
    "semver": "^7.5.4",
    "tough-cookie": "^4.1.3",
    "word-wrap": "^1.2.5",
    "vite": "^6.2.7",
    "cookie": "^0.7.2",
    "esbuild": "^0.20.1",
    "eslint": "^9.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@angular/build": {
      "vite": "^6.2.7",
      "esbuild": "^0.20.1"
    },
    "@eslint/config-array": "0.20.0",
    "@eslint/object-schema": "2.1.6",
    "@octokit/plugin-paginate-rest": "11.4.2",
    "@octokit/request": "9.2.1",
