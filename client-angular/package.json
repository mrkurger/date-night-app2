    "e2e:open": "ng serve client-angular --port 0 & cypress open",
    "lint": "eslint . --fix --max-warnings=9999",
    "lint:check": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=9999",
    "format": "prettier --write \"src/**/*.{ts,html,scss,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,css,json}\"",
    "format:all": "npm run format && npm run lint"
  },
  "private": true,
